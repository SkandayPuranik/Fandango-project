import numpy as np

mylist = [1, 2, 3, 4]
np.array(mylist)

my_matrix = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
print(np.array(my_matrix))

# Built in functions
print(np.arange(0, 10, 2))
print(np.zeros(5, 5))
print(np.linspace(0, 10, 11))
np.eye(5)  #identity matrix

np.random.rand(2, 5)
np.random.randn(10)

print(np.random.randint(0, 101, (4,5)))

rannar = np.random.randint(0, 101, 10)
rannar.max
rannar.min
rannar.argmax
rannar.argmin