import numpy as np
import pandas as pd

registrations = pd.DataFrame({'reg_id':[1,2,3,4],'name':['Andrew','Bobo','Claire','David']})
logins = pd.DataFrame({'log_id':[1,2,3,4],'name':['Xavier','Andrew','Yolanda','Bobo']})

# Inner merge
pd.merge(registrations, logins, how='inner', on='name')  # returns only the common instances

# Left and right merge
pd.merge(registrations, logins, how='left', on='name')  # returns everything from thr first table 
print(pd.merge(registrations, logins, how='right', on='name'))  # returns everything from thr first table 

# Outer merge
pd.merge(registrations, logins, how='outer', on='name')  # returns everything from both

